{
  "cSpell.words": [
    "brotli",
    "commitlint",
    "mantine",
    "offboard",
    "stylelint",
    "tabler",
    "tanstack",
    "Timesheet",
    "Trep"
  ],

  // "editor.defaultFormatter": "dbaeumer.vscode-eslint",
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.organizeImports": true
  },

  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ]
}
